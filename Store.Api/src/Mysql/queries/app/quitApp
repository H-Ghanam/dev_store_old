Select * FROM users WHERE id = 1;
SELECT * FROM stores WHERE active = 1 AND id IN (1)  ORDER BY sn;
SELECT cast('' as char(1)) TABLE_CATALOG, TABLE_SCHEMA, TABLE_NAME, COLUMN_NAME, ORDINAL_POSITION POSITION, DATA_TYPE, CHARACTER_MAXIMUM_LENGTH DATA_LENGTH, IF(DATETIME_PRECISION IS NULL, NUMERIC_PRECISION, DATETIME_PRECISION) DATA_PRECISION, NUMERIC_SCALE DATA_SCALE, (case IS_NULLABLE when 'YES' then 1 else 0 end) NULLABLE, COLUMN_DEFAULT DEFAULT_VALUE FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = 'sahl1' AND TABLE_NAME = 'stores' ORDER BY TABLE_SCHEMA, TABLE_NAME, ORDINAL_POSITION;
SELECT * FROM banks WHERE active = 1 AND id IN (2,1)  ORDER BY sn;
SELECT cast('' as char(1)) TABLE_CATALOG, TABLE_SCHEMA, TABLE_NAME, COLUMN_NAME, ORDINAL_POSITION POSITION, DATA_TYPE, CHARACTER_MAXIMUM_LENGTH DATA_LENGTH, IF(DATETIME_PRECISION IS NULL, NUMERIC_PRECISION, DATETIME_PRECISION) DATA_PRECISION, NUMERIC_SCALE DATA_SCALE, (case IS_NULLABLE when 'YES' then 1 else 0 end) NULLABLE, COLUMN_DEFAULT DEFAULT_VALUE FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = 'sahl1' AND TABLE_NAME = 'banks' ORDER BY TABLE_SCHEMA, TABLE_NAME, ORDINAL_POSITION;
SELECT id, title FROM users WHERE active = 1 ORDER BY sn;
SELECT cast('' as char(1)) TABLE_CATALOG, TABLE_SCHEMA, TABLE_NAME, COLUMN_NAME, ORDINAL_POSITION POSITION, DATA_TYPE, CHARACTER_MAXIMUM_LENGTH DATA_LENGTH, IF(DATETIME_PRECISION IS NULL, NUMERIC_PRECISION, DATETIME_PRECISION) DATA_PRECISION, NUMERIC_SCALE DATA_SCALE, (case IS_NULLABLE when 'YES' then 1 else 0 end) NULLABLE, COLUMN_DEFAULT DEFAULT_VALUE FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = 'sahl1' AND TABLE_NAME = 'users' ORDER BY TABLE_SCHEMA, TABLE_NAME, ORDINAL_POSITION;
SELECT * FROM stores order by sn;
FLUSH LOGS;
FLUSH TABLES;